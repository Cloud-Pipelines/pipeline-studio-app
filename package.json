{
  "name": "pipeline-editor",
  "homepage": "https://cloud-pipelines.net/pipeline-editor",
  "version": "0.1.0",
  "description": "Cloud Pipelines Editor is a web app that allows the users to build and run Machine Learning pipelines using drag and drop without having to set up development environment.",
  "keywords": [
    "ML",
    "AI",
    "Machine Learning",
    "pipelines",
    "containers",
    "cloud",
    "Kubernetes",
    "MLOps"
  ],
  "bugs": "https://github.com/Cloud-Pipelines/pipeline-editor/issues",
  "license": "Apache-2.0",
  "author": {
    "name": "Alexey Volkov",
    "email": "alexey.volkov+oss@ark-kun.com",
    "url": "http://ark-kun.com/"
  },
  "repository": "github:Cloud-Pipelines/pipeline-editor",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite --port 3000",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "test:coverage": "npx vitest --coverage",
    "lint": "eslint src --config eslint.config.js",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "gh-pages": "gh-pages -d dist",
    "lint:fix": "eslint src --config eslint.config.js --fix",
    "validate": "npm run format && npm run lint:fix && npm run typecheck",
    "validate:test": "npm run validate && npm run test"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-query": "^5.66.9",
    "@tanstack/react-query-devtools": "^5.66.9",
    "@tanstack/react-router": "^1.104.1",
    "@tanstack/router-devtools": "^1.104.3",
    "@types/gapi": "^0.0.39",
    "@xyflow/react": "^12.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gh-pages": "^6.3.0",
    "js-yaml": "^4.1.0",
    "localforage": "^1.10.0",
    "lucide-react": "^0.477.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-scan": "^0.2.4",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@testing-library/react": "^16.2.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.7",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "web-vitals": "^4.2.4"
  }
}
