{
  "name": "oasis-app",
  "homepage": "https://cloud-pipelines.net/pipeline-editor",
  "version": "0.1.0",
  "description": "Oasis is a web app that allows the users to build and run Machine Learning pipelines using drag and drop without having to set up development environment.",
  "keywords": [
    "ML",
    "AI",
    "Machine Learning",
    "pipelines",
    "containers",
    "cloud",
    "Kubernetes",
    "MLOps"
  ],
  "bugs": "https://github.com/Cloud-Pipelines/pipeline-studio-app/issues",
  "license": "Apache-2.0",
  "author": {
    "name": "Alexey Volkov",
    "email": "alexey.volkov+oss@ark-kun.com",
    "url": "http://ark-kun.com/"
  },
  "repository": "github:Cloud-Pipelines/pipeline-studio-app",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite --port 3000",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "test:coverage": "npx vitest --coverage",
    "lint": "eslint src --config eslint.config.js",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "gh-pages": "vite build && tsc && gh-pages -d dist",
    "lint:fix": "eslint src --config eslint.config.js --fix",
    "validate": "npm run format && npm run lint:fix && npm run typecheck",
    "validate:test": "npm run validate && npm run test",
    "generate-api": "npx @hey-api/openapi-ts -i http://localhost:8000/openapi.json -o src/api -c @hey-api/client-fetch",
    "knip": "knip"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/vite": "^4.1.10",
    "@tanstack/react-query": "^5.80.10",
    "@tanstack/react-query-devtools": "^5.80.10",
    "@tanstack/react-router": "^1.121.27",
    "@tanstack/router-devtools": "^1.121.27",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@xyflow/react": "^12.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "gh-pages": "^6.3.0",
    "js-yaml": "^4.1.0",
    "localforage": "^1.10.0",
    "lucide-react": "^0.523.0",
    "random-words": "^2.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-scan": "^0.3.4",
    "react-resizable-panels": "^3.0.3",
    "react-syntax-highlighter": "^15.6.1",
    "react-toastify": "^11.0.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.0.6",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/react": "^16.3.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "knip": "^5.61.2",
    "prettier": "^3.6.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^6.3.5",
    "vitest": "^3.0.5"
  }
}
